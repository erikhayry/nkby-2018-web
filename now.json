{
  "version": 2,
  "env": {
    "GOOGLE_MAPS_API": "@google_maps_api",
    "GOOGLE_STATIC_MAPS_API": "@google_static_maps_api"
  },
  "builds": [
    {
      "src": "lib/*.js",
      "use": "@now/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "next.config.js",
      "use": "@now/next@canary"
    },
    { "src": "static/**/*", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "alias": ["nkby.now.sh"]
}